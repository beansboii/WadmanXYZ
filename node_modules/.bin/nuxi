#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/nuxt@4.0.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_a7dbadc26fb1901cbe28a93667d9187f/node_modules/nuxt/bin/node_modules:/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/nuxt@4.0.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_a7dbadc26fb1901cbe28a93667d9187f/node_modules/nuxt/node_modules:/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/nuxt@4.0.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_a7dbadc26fb1901cbe28a93667d9187f/node_modules:/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/nuxt@4.0.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_a7dbadc26fb1901cbe28a93667d9187f/node_modules/nuxt/bin/node_modules:/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/nuxt@4.0.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_a7dbadc26fb1901cbe28a93667d9187f/node_modules/nuxt/node_modules:/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/nuxt@4.0.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_a7dbadc26fb1901cbe28a93667d9187f/node_modules:/Users/magicwren/Documents/GitHub/WadmanXYZ/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
