#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/sqlite-wasm/bin/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/sqlite-wasm/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/sqlite-wasm/bin/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/sqlite-wasm/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/sqlite-wasm/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../../@sqlite.org+sqlite-wasm@3.50.1-build1/node_modules/@sqlite.org/sqlite-wasm/bin/index.js" "$@"
fi
