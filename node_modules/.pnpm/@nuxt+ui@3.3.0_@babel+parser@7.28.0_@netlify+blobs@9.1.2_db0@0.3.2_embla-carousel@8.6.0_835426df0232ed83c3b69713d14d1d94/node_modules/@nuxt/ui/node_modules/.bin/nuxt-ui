#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules/@nuxt/ui/cli/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules/@nuxt/ui/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules/@nuxt/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules/@nuxt/ui/cli/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules/@nuxt/ui/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules/@nuxt/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+ui@3.3.0_@babel+parser@7.28.0_@netlify+blobs@9.1.2_db0@0.3.2_embla-carousel@8.6.0_835426df0232ed83c3b69713d14d1d94/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/index.mjs" "$@"
else
  exec node  "$basedir/../../cli/index.mjs" "$@"
fi
