#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.0.6_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_2dd795508664a5d5616edc0d3125aeda/node_modules/@nuxt/devtools/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.0.6_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_2dd795508664a5d5616edc0d3125aeda/node_modules/@nuxt/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.0.6_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_2dd795508664a5d5616edc0d3125aeda/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.0.6_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_2dd795508664a5d5616edc0d3125aeda/node_modules/@nuxt/devtools/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.0.6_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_2dd795508664a5d5616edc0d3125aeda/node_modules/@nuxt/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.0.6_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_2dd795508664a5d5616edc0d3125aeda/node_modules:/Users/magicwren/Desktop/wadman/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.mjs" "$@"
else
  exec node  "$basedir/../../cli.mjs" "$@"
fi
